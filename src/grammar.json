{
  "name": "jinja2",
  "rules": {
    "template": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "statement"
          },
          {
            "type": "SYMBOL",
            "name": "expression"
          },
          {
            "type": "SYMBOL",
            "name": "comment"
          },
          {
            "type": "SYMBOL",
            "name": "_text"
          }
        ]
      }
    },
    "statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{%"
        },
        {
          "type": "PATTERN",
          "value": "([^%]|%[^}])*%}"
        }
      ]
    },
    "expression": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{{"
        },
        {
          "type": "PATTERN",
          "value": "[^}}]*}}"
        }
      ]
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{#"
        },
        {
          "type": "PATTERN",
          "value": "([^#]|#[^}])*#+}"
        }
      ]
    },
    "_text": {
      "type": "PATTERN",
      "value": "."
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
